{
  "name": "qr-generator",
  "version": "1.0.0",
  "description": "A Node.js web application to generate QR codes with Angular frontend",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server.js",
    "client": "cd angular-app && npm run ng -- serve --port 4300",
    "build": "cd angular-app && ng build --output-path=../dist",
    "install-all": "npm install && cd angular-app && npm install"
  },
  "keywords": [
    "qr-code",
    "generator",
    "url",
    "web",
    "angular"
  ],
  "author": "Kevan",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "qrcode": "^1.5.3",
    "cors": "^2.8.5",
    "canvas": "^2.11.2",
    "multer": "^1.4.5-lts.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.2"
  }
}
